void bubbleSort(int arr[], int n) {
    for(int pass = 1; pass < n; pass++) {
        for(int i = 0; i < n - pass; i++) {
            if(arr[i] > arr[i + 1]) {
                int t = arr[i + 1];
                arr[i + 1] = arr[i];
                arr[i] = t;
            }
        }
    }
}
